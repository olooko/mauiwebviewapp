<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:viewModels="clr-namespace:MauiWebViewApp.ViewModels"
             x:DataType="viewModels:MainPageViewModel"
             x:Class="MauiWebViewApp.Views.MainPage">
    <ContentPage.Behaviors>
        <toolkit:EventToCommandBehavior EventName="Loaded" Command="{Binding NavigateCommand}" />
    </ContentPage.Behaviors>
    <Grid>
        <WebView Source="{Binding WebViewSource}" />
    </Grid>
</ContentPage>
